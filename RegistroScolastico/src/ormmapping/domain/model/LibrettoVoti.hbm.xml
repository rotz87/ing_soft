<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.model.LibrettoVoti" table="LibrettoVoti" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="libVotiLineItems" cascade="lock" column="LibrettoVotiLineItemID" class="domain.model.LibrettoVotiLineItem" not-null="true" lazy="no-proxy">
		</many-to-one>
	</class>
</hibernate-mapping>
