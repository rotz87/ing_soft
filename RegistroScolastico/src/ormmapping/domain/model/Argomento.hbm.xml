<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.model.Argomento" table="Argomento" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="_nome" column="_nome" type="string" length="255" not-null="false" lazy="false"/>
		<property name="_descrizione" column="_descrizione" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
