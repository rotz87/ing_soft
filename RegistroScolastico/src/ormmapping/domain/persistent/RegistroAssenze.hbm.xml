<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.persistent.RegistroAssenze" table="RegistroAssenze" schema="" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="appelloOdierno" column="AppelloIdAppello" class="domain.persistent.Appello" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy">
		</many-to-one>
		<set name="librettiAssenze" lazy="extra" cascade="save-update,lock" inverse="false">
			<key column="RegistroAssenzeID" not-null="true"/>
			<one-to-many class="domain.persistent.LibrettoAssenze"/>
		</set>
		<set name="appelli" lazy="extra" cascade="save-update,lock" inverse="false">
			<key column="RegistroAssenzeID" not-null="true"/>
			<one-to-many class="domain.persistent.Appello"/>
		</set>
	</class>
</hibernate-mapping>
