<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.persistent.UscitaAnticipata" table="UscitaAnticipata" schema="" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="appello" column="AppelloIdAppello" class="domain.persistent.Appello" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy">
		</many-to-one>
		<many-to-one name="permesso" column="PermessoUscitaID" class="domain.persistent.PermessoUscita" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy">
		</many-to-one>
	</class>
</hibernate-mapping>
